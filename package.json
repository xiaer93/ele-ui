{
  "name": "ele-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "loadsh": "^0.0.4",
    "postcss-color-mix": "^1.1.0",
    "postcss-current-selector": "^0.0.3",
    "postcss-nested-ancestors": "^2.0.0",
    "postcss-scss": "^2.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.22.12",
    "sass-loader": "^8.0.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "postcss-import": "^12.0.1",
    "precss": "^4.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "globals": {
      "_": "readonly"
    }
  },
  "postcss": {
    "syntax": "postcss-scss",
    "plugins": {
      "postcss-import": {},
      "postcss-nested-ancestors": {},
      "precss": {
        "stage": 0,
        "preserve": true,
        "features": {
          "color-mod-function": {
            "unresolved": "warn"
          }
        }
      },
      "postcss-color-mix": {},
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
